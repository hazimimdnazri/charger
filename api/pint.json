{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": true
        },
        "phpdoc_separation": false,
        "phpdoc_align": {
            "align": "left"
        },
        "single_line_empty_body": true,
        "php_unit_method_casing": {
            "case": "camel_case"
        }
    }
}